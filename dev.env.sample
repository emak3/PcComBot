# 開発用env（Analytics機能対応版）
PROFILE=develop

# YouTube API設定
YOUTUBE_ENABLED=true
YOUTUBE_API_KEY=
YOUTUBE_INTERVAL_MINUTES=9

# Discord Bot設定
TOKEN=
CLIENT_ID=
PCCOM_ID=
SIN_ID=

# Web設定
WEB_PORT=3000
WEB_DOMAIN=http://localhost:3000

CLIENT_SECRET=

# セッション暗号化キー生成: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
SESSION_SECRET=
# OAuth2設定
REDIRECT_URI=http://localhost:3000/auth/callback
# guilds+identify
AUTH_URL=

# 問い合わせ関連チャンネル設定
INQUIRY_CHANNEL_ID=
INQUIRY_CATEGORY_ID=

# 管理者ロール
ADMIN_ROLE_ID=
SUP_ROLE_ID=

# Discord招待リンク
DISCORD_INVITE_URL=https://discord.gg/uxENZNrk5n

# =================================
# Firebase設定（Analytics機能に必要）
# =================================

# Firebase プロジェクト設定
FIREBASE_PROJECT_ID=
FIREBASE_DATABASE_URL=

# Firebase サービスアカウント認証（必須）
FIREBASE_PRIVATE_KEY=""
FIREBASE_CLIENT_EMAIL=

# Firebase サービスアカウント設定（オプション）
FIREBASE_PRIVATE_KEY_ID=
FIREBASE_CLIENT_ID=
FIREBASE_CLIENT_X509_CERT_URL=

# =================================
# RSS機能設定
# =================================
RSS_ENABLED=true
RSS_INTERVAL_MINUTES=10

# =================================
# ログ設定
# =================================
LOG_LEVEL=debug
LOG_FILE_OUTPUT=true
LOG_MAX_FILE_SIZE=10MB
LOG_MAX_FILES=5

# =================================
# 開発者設定
# =================================
NODE_ENV=development
DEBUG_MODE=true
MOCK_DATA=false